{
    "manifest_version": 2,
    "name": "検索TLから集計するスクリプト",
    "author": "綿風コウラン",
    "version": "1.0",
    "description": "いいね順に集めたかったので",
    "permissions": [
        "tabs",
        "background",
        "http://*/*",
        "https://*/*"
    ],
    "background": {
        "scripts": ["background.js"]
    },
    "browser_action": {
    },
    "content_scripts": [
    {
        "matches": [ "https://twitter.com/*" ],
        "js": [
            "script.js"
        ]
    }],
    "web_accessible_resources": [
        "tweet_data_script.js"
    ]
}